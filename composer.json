{
  "name": "rcm/api-lib",
  "description": "Rcm Api Library",
  "type": "library",
  "license": "BSD-3-Clause",
  "keywords": [
    "rcm",
    "module",
    "zf2",
    "api"
  ],
  "minimum-stability": "stable",
  "homepage": "https://github.com/reliv/rcm-api-lib",
  "authors": [
    {
      "name": "James Jervis",
      "email": "jjervis@relivinc.com",
      "homepage": "https://github.com/reliv"
    }
  ],
  "require": {
    "php": ">=5.5",
    "zendframework/zendframework": "2.*",
    "rwoverdijk/assetmanager": "1.*",
    "fxp/composer-asset-plugin": "^1.2.0",
    "rcm/i18n": "1.*",
    "bower-asset/rcm-js-lib": "0.*"
  },
  "autoload": {
    "psr-4": {
      "Reliv\\RcmApiLib\\": "src/"
    }
  },
  "extra": {
    "asset-pattern-skip-version": "(-patch*)",
    "asset-vcs-driver-options": {
      "github-no-api": true
    }
  }
}
